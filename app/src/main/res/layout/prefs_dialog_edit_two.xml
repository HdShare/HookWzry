<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical"    android:paddingStart="?android:attr/dialogPreferredPadding"    android:paddingTop="?android:attr/dialogPreferredPadding"    android:paddingEnd="?android:attr/dialogPreferredPadding">    <com.google.android.material.textfield.TextInputLayout        android:id="@+id/prefs_dialog_edit_two_layout1"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="6dp">        <com.google.android.material.textfield.TextInputEditText            android:id="@+id/prefs_dialog_edit_two_edt1"            android:layout_width="match_parent"            android:layout_height="wrap_content"            tools:ignore="VisualLintTextFieldSize"            tools:text="prefs_edt1" />    </com.google.android.material.textfield.TextInputLayout>    <com.google.android.material.textfield.TextInputLayout        android:id="@+id/prefs_dialog_edit_two_layout2"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="6dp">        <com.google.android.material.textfield.TextInputEditText            android:id="@+id/prefs_dialog_edit_two_edt2"            android:layout_width="match_parent"            android:layout_height="wrap_content"            tools:ignore="VisualLintTextFieldSize"            tools:text="prefs_edt2" />    </com.google.android.material.textfield.TextInputLayout></LinearLayout>